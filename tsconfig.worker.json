{
	"extends": "./tsconfig.node.json",
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
		"types": [
			"@cloudflare/workers-types",
			"./worker-configuration.d.ts",
			"vite/client",
			"jest"
		],
		"lib": [
			"ES2023"
		],
		"paths": {
			"shared/*": [
				"./shared/*"
			],
			"worker/*": [
				"./worker/*"
			],
			"@vibesdk/design-mode-client": [
				"./packages/@vibesdk/design-mode-client/dist/index.d.ts"
			],
			"@vibesdk/design-mode-client/embedded": [
				"./packages/@vibesdk/design-mode-client/dist/embedded.d.ts"
			]
		}
	},
	"include": [
		"./worker-configuration.d.ts",
		"./shared",
		"./worker",
		"./worker/types"
	],
	"exclude": [
		"**/__tests__/**/*",
		"**/*.test.ts"
	]
}